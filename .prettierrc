{
  "arrowParens":  "always",
  "bracketSpacing":  true,
  "insertPragma":  false,
  "printWidth":  90,
  "proseWrap":  "always",
  "quoteProps":  "as-needed",
  "requirePragma":  false,
  "semi":  true,
  "singleQuote":  true,
  "tabWidth":  2,
  "trailingComma":  "all",
  "useTabs":  false,
  "plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-sort-json"
	]
}
